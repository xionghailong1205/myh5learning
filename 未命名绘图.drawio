        <mxCell id="q_WZgCHlJQGWYBV_8L__-3" value="&lt;font style=&quot;font-size: 24px;&quot;&gt;dispaly&lt;/font&gt;" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
